{
  "name": "marcin-orlowski/laravel-api-response-builder",
  "description": "Helps building nice, normalized and easy to consume Laravel REST API.",
  "homepage": "https://github.com/MarcinOrlowski/laravel-api-response-builder",
  "keywords": [
    "laravel",
    "json",
    "api",
    "builder",
    "rest",
    "helper",
    "php7",
    "mobile",
    "android",
    "ios"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcin Orlowski",
      "homepage": "http://MarcinOrlowski.com/"
    }
  ],
  "support": {
    "source": "https://github.com/MarcinOrlowski/laravel-api-response-builder/",
    "issues": "https://github.com/MarcinOrlowski/laravel-api-response-builder/issues"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MarcinOrlowski\\ResponseBuilder\\ResponseBuilderServiceProvider"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "src/",
      "tests/",
      "tests/Traits/"
    ]
  },
  "require": {
    "php": ">=7.2.0",
    "laravel/framework": ">= 6.0"
  },
  "require-dev": {
    "marcin-orlowski/coding-standard": "^1.3",
    "marcin-orlowski/phpunit-extra-asserts": "^1.2.0",
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0",
    "phpunit/php-code-coverage": "^7.0",
    "friendsofphp/php-cs-fixer": "^2.15",
    "nunomaduro/larastan": "^0.7.6"
  }
}
